A = LOAD '/home/cloudera/Desktop/weatherdataset.txt' USING PigStorage(' ') AS (Year:int,Temp:int);
B= ORDER A BY Temp DESC;
C = LIMIT B 1;
D = FOREACH C GENERATE Temp;
DUMP D;

